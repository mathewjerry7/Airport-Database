SQL> CREATE TABLE AIRPORT( 2 AIRPORT_CODE varchar(30), 3 NAME varchar(30),
4 CITYvarchar(30),
5 STATE varchar(30) 6 );
SQL> CREATE TABLE FLIGHT( 2 FNUMBER varchar(30),
3 AIRLINE varchar(30),
4 WEEKDAYS varchar(30)
5 );
SQL> CREATE TABLE FLIGHT_LEG(
2 FLIGHT_NUMBER varchar(30),
3 LEG_NUMBER varchar(30),
4 DEPARTURE_AIRPORT_CODE varchar(30),
5 SCHEDULED_DEPARTURE_TIME timestamp, 6 ARRIVAL_AIRPORT_CODE varchar(30),
7 SCHEDULED_ARRIVAL_TIME timestamp 8 );
SQL> CREATE TABLE LEG_INSTANCE( 2 FLIGHT_NUMBER varchar(30),
3 LEG_NUMBER varchar(30),
4 FDATE varchar(30),
5 NUMBER_OF_AVAILABLE_SEATS varchar(30), 6 AIRPLANE_ID varchar(30),
7 DEPARTURE_AIRPORT_CODE varchar(30),
8 DEPARTURE_TIME timestamp,
9 ARRIVAL_AIRPORT_CODE varchar(30), 10 ARRIVAL_TIME varchar(30)
11 );
SQL> CREATE TABLE FARES(
2 FLIGHT_NUMBER varchar(30), 3 FARE_CODE varchar(30),
4 AMOUNT varchar(30),
5 RESTRICTIONS varchar(30)
6 );
SQL> CREATE TABLE AIRPLANE_TYPE( 2 TYPE_NAME varchar(30),
3 MAX_SEATS varchar(30),
4 COMPANYvarchar(30)
5 );
SQL> CREATE TABLE CAN_LAND(
2 AIRPLANE_TYPE_NAME varchar(30), 3 AIRPORT_CODE varchar(30)
4 );
SQL> CREATE TABLE AIRPLANE(
2 AIRPLANE_ID varchar(30),
3 TOTAL_NUMBER_OF_SEATS varchar(30), 4 AIRPLANE_TYPE varchar(30)
5 );
SQL> CREATE TABLE SEAT_RESERVATION( 2 FLIGHT_NUMBER varchar(30),
3 LEG_NUMBER varchar(30),
4 FDATE varchar(30),
5 SEAT_NUMBER varchar(30),
6 CUSTOMER_CARE varchar(30), 7 CUSTOMER_PHONE varchar(30) 8 );
